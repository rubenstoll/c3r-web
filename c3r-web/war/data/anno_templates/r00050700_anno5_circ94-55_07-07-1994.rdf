        il existe SignalisationAdaptee à CabinetAisanceAmenageHandicapees: CabinetAisanceAmenageHandicapee hasSignalisation SignalisationAdaptee 
    </interpretationExpert>
    
    <contenuRequete rdf:ID="r00050700q1">
        PREFIX ontoCC: &lt;http://www.owl-ontologies.com/Ontology1205837312.owl#&gt;
        select ?sign ?cabinet display xml 
        where
        {
        ?cabinet rdf:type ontoCC:CabinetAisance 
        ?cabinet ontoCC:hasSignalisation ?sign
        ?sign	rdf:type 		ontoCC:Signalisation
        OPTIONAL 
        {
        ?sign	rdf:type		 ?t
        FILTER ( ?t = ontoCC:SignalisationAdaptee)
        }
        FILTER ( ! bound( ?t ) )	
        }
    </contenuRequete> 
    
