    </cos:rule>
    
    <cos:rule rdf:ID="EspaceManoeuvrePorte">
        <cos:if>
            PREFIX ontoCC: &lt;http://www.owl-ontologies.com/Ontology1205837312.owl#&gt;
           {
           {
            ?x            rdf:type    ontoCC:EspaceManoeuvre 
            ?relsb    rdf:type    ontoCC:IfcRelSpaceBoundary
            ?relsb    ontoCC:relatingSpace    ?x
            ?relsb    ontoCC:relatedBuildingElement    ?op
            ?op        rdf:type    ontoCC:IfcOpeningElement
            ?relf        rdf:type    ontoCC:IfcRelFillsElement
            ?relf        ontoCC:relatedBuildingElement    ?op
            ?relf        ontoCC:relatingOpeningElement    ?door
            ?door    rdf:type    ontoCC:IfcDoor
            }
            UNION
            {
            ?x            rdf:type    ontoCC:EspaceManoeuvre 
            ?relcon    rdf:type    ontoCC:IfcRelSpaceBoundary  
            ?relcon    ontoCC:relatingSpace    ?x
            ?relcon    ontoCC:relatedBuildingElement    ?door
            ?door    rdf:type    ontoCC:IfcDoor  
            }
            }
        </cos:if>
        
        <cos:then>
            { 
            ?x rdf:type ontoCC:EspaceManoeuvrePorte .
            }
        </cos:then>
    </cos:rule>
    
    <cos:rule rdf:ID="EspaceManoeuvrePossibiliteDemiTour">
        <cos:if>
            PREFIX ontoCC: &lt;http://www.owl-ontologies.com/Ontology1205837312.owl#&gt;
            {
            ?x     rdf:type    ontoCC:EspaceManoeuvre 
            }
        </cos:if>
        
        <cos:then>
