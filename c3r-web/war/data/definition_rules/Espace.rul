        </cos:then>
    </cos:rule>
    
    <cos:rule rdf:ID="EspaceCommunication">
        <cos:if>
            PREFIX ontoCC: &lt;http://www.owl-ontologies.com/Ontology1205837312.owl#&gt;
            {
            ?x     rdf:type    ontoCC:Espace 
            ?x    ontoCC:destination ?t
            FILTER (?t ~ 'communication')
            }
        </cos:if>
        
        <cos:then>
            { ?x rdf:type ontoCC:EspaceCommunication .}
        </cos:then>
    </cos:rule>
    
 </rdf:RDF>
